	"main": "index.js",
	"packageManager": "pnpm@9.4.0",
	"engines": {
		"node": ">=v20.18.0"
	},
	"scripts": {
		"build": "turbo run build",
		"clean": "git clean -xdf node_modules .turbo rm pnpm-lock.yaml",
		"clean:workspaces": "turbo run clean",
		"db:generate": "turbo run generate --filter=@qarn-nutritionist/db",
