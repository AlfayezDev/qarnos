{
  "name": "qarn",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": ">=v22.8.0"
  },
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules .turbo rm pnpm-lock.yaml",
    "clean:workspaces": "turbo run clean",
    "db:generate": "turbo run generate --filter=@qarn-nutritionist/db",
    "dev": "turbo watch dev",
    "format": "turbo format --continue --",
    "format:fix": "turbo format --continue -- --write",
    "lint": "turbo lint --continue --",
    "lint:fix": "turbo lint --continue -- --apply",
    "lint:ws": "pnpm dlx sherif@latest",
    "typecheck": "turbo run typecheck",
    "noly": "pnpm dlx nolyfill",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "catalog:"
  },
  "dependencies": {
    "expo": "~51.0.28",
    "react": "18.2.0",
    "react-native": "0.74.5"
  }
}
